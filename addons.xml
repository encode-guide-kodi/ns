<addons>
<addon id="plugin.program.nswiz" name="NS Wizard" version="0.0.1" provider-name="encode">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">NS Wizard</summary>
    <description lang="en">NS Wizard for Natho's Subs.</description>
    <platform>all</platform>
  </extension>
</addon>
<addon id="script.tvguide.ns" name="NS Guide"
version="0.1.1"
provider-name="encode">
    <requires>
        <import addon="xbmc.python" version="2.25.0"/>
        <import addon="script.module.simplejson" version="3.3.0"/>
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.dateutil" version="2.4.2" />
        <import addon="script.module.pil" />
        <import addon="script.module.beautifulsoup4" />
    </requires>
    <extension point="xbmc.python.script" library="addon.py">
            <provides>executable</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="login"/>
    <extension point="xbmc.addon.metadata">
        <description lang="en">This Addon comes as a bonus!, neither the dev nor Natho Subs offer support,
		Based on the original TVGF addon by Primaeval, IPTV Integration is at your own risk!</description>
        <disclaimer lang="en"></disclaimer>
        <language></language>
        <platform>all</platform>
        <license>GPLv2</license>
        <forum></forum>
        <source></source>
        <website></website>
        <email></email>
        <news>
        </news>
    </extension>
</addon>
